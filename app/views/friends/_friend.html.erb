<tr id="<%= dom_id friend %>">
  <td>
    <%= link_to friend.first_name + ' ' + friend.last_name , friend %>
  </td>
  <td>
    <%= friend.email %>
  </td>
  <td>
    <%= friend.phone %>
  </td>
  <td>
    <%= friend.user_id %>
  </td>
  <td>
    <%= button_to 'Delete', friend, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-outline-danger btn-sm" %>
  </td>
</tr>

